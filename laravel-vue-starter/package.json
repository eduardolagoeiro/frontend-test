{
    "name": "appwork-laravel-vue-starter",
    "version": "1.3.1",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "~7.2.0",
        "core-js": "2",
        "cross-env": "~5.2.0",
        "laravel-mix": "~4.0.15",
        "node-sass": "~4.14.1",
        "resolve-url-loader": "2.3.1",
        "sass-loader": "7.*",
        "sass": "~1.17.3",
        "vue-template-compiler": "^2.6.9",
        "axios": "~0.18.0",
        "bootstrap": "~4.3.1",
        "bootstrap-vue": "2.0.0-rc.16",
        "chart.js": "~2.8.0",
        "echarts": "^4.2.1",
        "idle-vue": "~2.0.5",
        "ladda": "~2.0.1",
        "moment": "~2.24.0",
        "nouislider": "~13.1.4",
        "numeral": "~2.0.6",
        "perfect-scrollbar": "~1.4.0",
        "plyr": "~2.0.18",
        "simplemde": "~1.11.2",
        "spinkit": "~1.2.5",
        "sweet-modal-vue": "~2.0.0",
        "text-mask-addons": "~3.8.0",
        "v-img": "~0.2.0",
        "vue": "~2.6.6",
        "vue-awesome-swiper": "~3.1.3",
        "vue-blockui": "~1.1.6",
        "vue-chartjs": "~3.4.0",
        "vue-clipboard2": "~0.3.0",
        "vue-color": "~2.7.0",
        "vue-context-menu": "~2.0.6",
        "vue-dragula": "~1.3.1",
        "vue-echarts": "~4.0.1",
        "vue-flatpickr-component": "~8.1.2",
        "vue-form-wizard": "~0.8.4",
        "vue-gallery": "~2.0.0",
        "vue-input-tag": "~2.0.5",
        "vue-jstree": "~2.1.6",
        "vue-masonry": "~0.11.1",
        "vue-meta": "~1.6.0",
        "vue-multiselect": "~2.1.0",
        "vue-notification": "~1.3.7",
        "vue-number-input-spinner": "~2.1.0",
        "vue-password-strength-meter": "~1.4.2",
        "vue-quill-editor": "~3.0.5",
        "vue-router": "~3.0.1",
        "vue-simple-calendar": "~4.2.1",
        "vue-simplemde": "~0.5.1",
        "vue-slider-component": "~3.0.16",
        "vue-tables-2": "~1.4.50",
        "vue-text-mask": "~6.1.1",
        "vue-toasted": "~1.1.24",
        "vue-typeahead": "~2.3.2",
        "vue-upload-component": "~2.8.3",
        "vue2-dropzone": "3.5.2",
        "vue2-google-maps": "~0.10.2",
        "vuedraggable": "2.20.0",
        "vuejs-datepicker": "~1.5.3",
        "vuetrend": "~0.3.2",
        "zxcvbn": "~4.4.2"
    },
    "browserslist": [
        ">= 1%",
        "last 2 versions",
        "not dead",
        "Chrome >= 45",
        "Firefox >= 38",
        "Edge >= 12",
        "Explorer >= 10",
        "iOS >= 9",
        "Safari >= 9",
        "Android >= 4.4",
        "Opera >= 30"
    ],
    "babel": {
        "presets": [
            ["@babel/env", {
                "targets": {
                    "browsers": [
                        ">= 1%",
                        "last 2 versions",
                        "not dead",
                        "Chrome >= 45",
                        "Firefox >= 38",
                        "Edge >= 12",
                        "Explorer >= 10",
                        "iOS >= 9",
                        "Safari >= 9",
                        "Android >= 4.4",
                        "Opera >= 30"
                    ]
                }
            }]
        ],
        "plugins": ["@babel/plugin-syntax-dynamic-import"]
    }
}
