<% content_for :styles do %>
  <%= stylesheet_link_tag 'flot/flot.css', media: 'all' %>
  <%= stylesheet_link_tag 'c3/c3.css', media: 'all' %>
  <%= stylesheet_link_tag 'chartist/chartist.css', media: 'all' %>
  <%= stylesheet_link_tag 'morris/morris.css', media: 'all' %>
<% end %>

<% content_for :scripts do %>
  <!-- Dependencies -->
  <!-- NOTE: When using Google Maps on your own site you MUST get your own API key:
             https://developers.google.com/maps/documentation/javascript/get-api-key
             After you got the key paste it in the URL below. -->
  <script src="https://maps.google.com/maps/api/js?key=AIzaSyCHtdj4L66c05v1UZm-nte1FzUEAN6GKBI"></script>
  <%= javascript_include_tag 'gmaps/gmaps.js' %>
  <%= javascript_include_tag 'eve/eve.js' %>
  <%= javascript_include_tag 'raphael/raphael.js' %>
  <%= javascript_include_tag 'mapael/mapael.js' %>
  <%= javascript_include_tag 'mapael/maps/world_countries.js' %>
  <%= javascript_include_tag 'flot/flot.js' %>
  <%= javascript_include_tag 'd3/d3.js' %>
  <%= javascript_include_tag 'c3/c3.js' %>
  <%= javascript_include_tag 'chartist/chartist.js' %>
  <%= javascript_include_tag 'chartjs/chartjs.js' %>
  <%= javascript_include_tag 'morris/morris.js' %>
  <%= javascript_include_tag 'sparkline/sparkline.js' %>
<% end %>

<!--
=================================================================
**************************** CONTENT ****************************
-->

<%= render "complete_ui/gmaps" %>
<%= render "complete_ui/mapael" %>
<%= render "complete_ui/flot" %>
<%= render "complete_ui/c3" %>
<%= render "complete_ui/chartist" %>
<%= render "complete_ui/chartjs" %>
<%= render "complete_ui/morris" %>
<%= render "complete_ui/sparkline" %>
