        if (typeof document.documentMode !== 'number' || document.documentMode > 10) {
            document.open();
            @Scripts.RenderFormat(@"document.write('\x3Cscript src=""{0}"">\x3C/script>');", "~/bundle/vendor/js/quill/quill")
            document.close();
        }
    </script>
