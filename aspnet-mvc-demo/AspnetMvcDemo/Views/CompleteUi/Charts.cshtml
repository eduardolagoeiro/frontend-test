@{
    ViewBag.Title = "Charts";
    Layout = "~/Views/Shared/Layouts/_CompleteUi.cshtml";
}

@section Styles {
    @Styles.Render("~/bundle/vendor/css/flot/flot")
    @Styles.Render("~/bundle/vendor/css/c3/c3")
    @Styles.Render("~/bundle/vendor/css/chartist/chartist")
    @Styles.Render("~/bundle/vendor/css/morris/morris")
}

@section Scripts {
    <!-- Dependencies -->
    <!-- NOTE: When using Google Maps on your own site you MUST get your own API key:
                         https://developers.google.com/maps/documentation/javascript/get-api-key
                         After you got the key paste it in the URL below. -->
    <script src="https://maps.google.com/maps/api/js?key=AIzaSyCHtdj4L66c05v1UZm-nte1FzUEAN6GKBI"></script>
    @Scripts.Render("~/bundle/vendor/js/gmaps/gmaps")
    @Scripts.Render("~/bundle/vendor/js/eve/eve")
    @Scripts.Render("~/bundle/vendor/js/raphael/raphael")
    @Scripts.Render("~/bundle/vendor/js/mapael/mapael")
    @Scripts.Render("~/bundle/vendor/js/mapael/maps/world_countries")
    @Scripts.Render("~/bundle/vendor/js/flot/flot")
    @Scripts.Render("~/bundle/vendor/js/d3/d3")
    @Scripts.Render("~/bundle/vendor/js/c3/c3")
    @Scripts.Render("~/bundle/vendor/js/chartist/chartist")
    @Scripts.Render("~/bundle/vendor/js/chartjs/chartjs")
    @Scripts.Render("~/bundle/vendor/js/morris/morris")
    @Scripts.Render("~/bundle/vendor/js/sparkline/sparkline")
}


<!--
=================================================================
**************************** CONTENT ****************************
-->

@Html.Partial("~/Views/CompleteUi/Gmaps.cshtml")
@Html.Partial("~/Views/CompleteUi/Mapael.cshtml")
@Html.Partial("~/Views/CompleteUi/Flot.cshtml")
@Html.Partial("~/Views/CompleteUi/C3.cshtml")
@Html.Partial("~/Views/CompleteUi/Chartist.cshtml")
@Html.Partial("~/Views/CompleteUi/Chartjs.cshtml")
@Html.Partial("~/Views/CompleteUi/Morris.cshtml")
@Html.Partial("~/Views/CompleteUi/Sparkline.cshtml")